{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": ["prettier", "prettier/flowtype", "prettier/react"],
  "plugins": ["flowtype", "prettier", "promise", "react", "import"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}
